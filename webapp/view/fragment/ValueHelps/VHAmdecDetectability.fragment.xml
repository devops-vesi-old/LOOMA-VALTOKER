<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="idEquipAmdecDetectabilityDialog"
        noDataText="{i18n>noDataFound}"
        items="{
            path: 'mVH>/AmdecDetectability',
            templateShareable: true
        }"
        search="onAmdecSelectDialogSearch"
        confirm="onAmdecVhSelectDialogConfirm($event, 'AmdecDetectabilityId')"
        cancel="onValueHelpClose"
    >
        <StandardListItem title="{mVH>ValueId} - {mVH>ValueDesc}" />
    </SelectDialog>
</core:FragmentDefinition>