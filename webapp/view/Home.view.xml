<mvc:View 
	controllerName="com.vesi.zfioac4_valpec.controller.Home" 
	displayBlock="true" 
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:semantic="sap.f.semantic">
	<semantic:SemanticPage>
		<semantic:titleHeading>
			<Title text="{i18n>homePageHeaderTitle}"/>
		</semantic:titleHeading>
		<semantic:headerContent>
			<core:Fragment 
				fragmentName="com.vesi.zfioac4_valpec.view.fragment.Home.FilterBar" 
				type="XML"/>
		</semantic:headerContent>
		<semantic:content>
			<core:Fragment 
				fragmentName="com.vesi.zfioac4_valpec.view.fragment.Home.Table" 
				type="XML"/>
		</semantic:content>
	</semantic:SemanticPage>
</mvc:View>